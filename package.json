{
  "name": "lan-stack",
  "version": "1.0.0",
  "description": "QuickStart for lan applicaitons",
  "main": "server/server.js",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": "8.10.0"
  },
  "scripts": {
    "start": "node .",
    "postinstall": "npm run build",
    "buildEnv": "node ./buildEnv.js",
    "build": "npm run buildEnv && npm run build:client && npm run build:server",
    "serve": "npm run buildEnv && ng serve",
    "build:client": "ng build --prod",
    "build:server": "ng build --prod --app 1 --output-hashing=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:server": "nyc mocha server/test/**/*.test.js --timeout 10000",
    "test:client": "ng test --code-coverage --reporters=coverage-istanbul",
    "test": "npm run test:server && npm run test:client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RadialSpark/LAN-Stack"
  },
  "author": "RadialSpark",
  "bugs": {
    "url": "https://github.com/RadialSpark/LAN-Stack/issues"
  },
  "homepage": "https://github.com/RadialSpark/LAN-Stack#readme",
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/platform-server": "^5.2.8",
    "@angular/router": "^5.2.0",
    "@nguniversal/express-engine": "^5.0.0-beta.6",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.6",
    "compression": "^1.0.3",
    "core-js": "^2.4.1",
    "cors": "^2.5.2",
    "helmet": "^1.3.0",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^5.0.0",
    "rxjs": "^5.5.6",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^2.0.0",
    "throng": "^4.0.0",
    "ts-loader": "^4.0.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "codelyzer": "^4.0.1",
    "eslint": "^3.17.1",
    "eslint-config-loopback": "^8.0.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "3.2.0",
    "nsp": "^2.1.0",
    "nyc": "^11.6.0",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 75,
    "statements": 75,
    "functions": 75,
    "branches": 75,
    "include": [
      "server/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "server/test/**/*.js",
      "server/node_modules/**/*",
      "server/datasources.*",
      "server/middleware.*",
      "server/utils/constants.js",
      "server/config.*",
      "server/server.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [],
    "extension": [
      ".jsx"
    ],
    "cache": true,
    "all": true
  }
}
